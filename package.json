{
  "name": "astro-learning",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 astro dev",
    "build": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 astro build",
    "preview": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 astro preview",
    "astro": "astro",
    "deploy:gh": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 BASE_URL=/horns-n-hooves/ astro build && gh-pages -d dist -b gh-pages -r",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report"
  },
  "dependencies": {
    "@astrojs/preact": "^4.1.3",
    "astro": "^5.15.3",
    "chart.js": "^4.5.1",
    "node-cache": "^5.1.2",
    "preact": "^10.27.2",
    "react-chartjs-2": "^5.3.1",
    "recharts": "^3.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "cross-env": "^7.0.3"
  }
}